Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/githubKey

Host hpc
  HostName rider.case.edu
  User CaseId
  IdentityFile ~/.ssh/id_hpc_ed25519

Host hpc-pioneer
  HostName pioneer.case.edu
  User CaseId
  IdentityFile ~/.ssh/pioneerKey


# The next setup is useful only in interactive mode.
# If you want to use GUI in your connection consider it, otherwise use ssh w/o x11 flag.
Host hpc-x11
  HostName rider.case.edu
  User CaseId
  IdentityFile ~/.ssh/id_hpc_ed25519
  ForwardX11 yes


